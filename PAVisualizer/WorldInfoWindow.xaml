<Window x:Class="PAVisualizer.WorldInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PAVisualizer"
        d:DataContext="{d:DesignInstance Type=local:WorldInfoWindow}"
        mc:Ignorable="d"
        Title="World info" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="worldInfoTextBox" IsReadOnly="True"></TextBox>
    </Grid>
</Window>
